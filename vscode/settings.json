{
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "editor.fontLigatures": false,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Liberation Mono','JetBrains Mono'",
    "diffEditor.ignoreTrimWhitespace": true,
    "aws.telementry": false,
    "amazonQ.telementry": false,
    "editor.wordWrap": "off",
    "dotnet.codeLens.enableReferencesCodeLens": false,
    "dotnet.codeLens.enableTestCodeLens": false,
    "editor.semanticHighlighting.enabled": false,
    "editor.links": false,
    "editor.fontWeight": "normal",
    "workbench.colorTheme": "chalice-color-theme-dark",
    "explorer.autoOpenDroppedFile": true
}
